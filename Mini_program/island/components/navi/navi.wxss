/* components/navi/navi.wxss */
.container {
  width: 80%;
  display: flex;
  margin: 0 auto;
  background-color: #f3f3f3;
  border-radius: 5rpx;
}
.container .left {
  flex: 1;
  width: 80rpx;
  height: 80rpx;
}
.container .title {
  flex: 7;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
}
.container .right {
  flex: 1;
  width: 80rpx;
  height: 80rpx;
}
